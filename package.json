  "scripts": {
    "test": "ts-mocha src/**/*.spec.ts src/test.spec.ts",
    "test:cc": "./node_modules/nyc/bin/nyc.js ts-mocha src/**/*.spec.ts src/test.spec.ts",
    "dev": "ts-node ./src/server.ts",
    "prettier": "prettier --write src/**/*.{ts,js,css,html}",
    "prepare": "husky install",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:globals": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s 'CreateGlobalSettings'",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "reseed": "npm run schema:drop && npm run seed:run",
    "lint": "eslint -c .eslintrc.json --ext .ts,.js src/"
  },
  "keywords": [],
